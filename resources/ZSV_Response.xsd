																				<xs:documentation>Результаты проверки</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:attribute name="КодРезПроверки" use="required">
																					<xs:annotation>
																						<xs:documentation>Код в соответствии с перечнем кодов результатов проверки, допустимых для подтверждения</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="33"/>
																								<altova:example value="32"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:length value="2"/>
																							<xs:pattern value="\d{2}"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																				<xs:attribute name="Пояснение" use="optional">
																					<xs:annotation>
																						<xs:documentation>Текст, уточняющий причины отрицательного результата проверки</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="ИНН клиента не соответствует номеру счета"/>
																								<altova:example value="в банке отсутствует номер счета, указанный в электронном документе"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:minLength value="1"/>
																							<xs:maxLength value="512"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																				<xs:attribute name="КодРекв" use="optional">
																					<xs:annotation>
																						<xs:documentation>Код ошибочного реквизита</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="ИНННП"/>
																								<altova:example value="НомСч"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:minLength value="1"/>
																							<xs:maxLength value="50"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																				<xs:attribute name="ЗначРекв" use="optional">
																					<xs:annotation>
																						<xs:documentation>Значение ошибочного реквизита</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="5208000760"/>
																								<altova:example value="40702810100000002456"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:minLength value="1"/>
																							<xs:maxLength value="1000"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="Операции" minOccurs="0" maxOccurs="1000">
																			<xs:annotation>
																				<xs:documentation>Сведения по операциям на счете (специальном банковском счете), по операциям по вкладу (депозиту), о переводах остатка электронных денежных средств</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="РеквДок">
																						<xs:annotation>
																							<xs:documentation>Реквизиты документа, на основании которого была совершена операция по счету, вкладу (депозиту), был осуществлен перевод</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:attribute name="ВидДок" use="optional">
																								<xs:annotation>
																									<xs:documentation>Вид (шифр) документа</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="11"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:nonNegativeInteger">
																										<xs:totalDigits value="2"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="НомДок" use="required">
																								<xs:annotation>
																									<xs:documentation>Номер документа, на основании которого была совершена операция, перевод</xs:documentation>
																									<xs:documentation>
Дата в формате ДД.ММ.ГГГГ</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="355/23-Т"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="20"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="ДатаДок" type="xs:date" use="required">
																								<xs:annotation>
																									<xs:documentation>Дата документа</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="2010-03-15"/>
																											<altova:example value="2013-04-21"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																							</xs:attribute>
																						</xs:complexType>
																					</xs:element>
																					<xs:element name="РеквБанка">
																						<xs:annotation>
																							<xs:documentation>Реквизиты банка плательщика/получателя денежных средств (ЭСП)</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:attribute name="НомКорСч" use="optional">
																								<xs:annotation>
																									<xs:documentation>Номер корреспондентского счета банка (иностранного банка)</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="30101810600000000957"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="34"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="НаимБП" use="required">
																								<xs:annotation>
																									<xs:documentation>Наименование банка</xs:documentation>
																									<xs:documentation>
Дата в формате ДД.ММ.ГГГГ</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="Банк ВТБ 24"/>
																											<altova:example value="Сосьете Женераль"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="160"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="БИКБП" use="required">
																								<xs:annotation>
																									<xs:documentation>БИК по справочнику БИК России или код банка-нерезидента по SWIFT или национальный код банка</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="046015262"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="34"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																						</xs:complexType>
																					</xs:element>
																					<xs:element name="РеквПлат">
																						<xs:annotation>
																							<xs:documentation>Реквизиты плательщика/получателя денежных средств (ЭСП)</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:attribute name="НаимПП" use="required">
																								<xs:annotation>
																									<xs:documentation>Наименование организации или ФИО плательщика</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="ОАО Ромашка"/>
																											<altova:example value="Депардье Жерар Филипович"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="160"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="ИННПП" use="optional">
																								<xs:annotation>
																									<xs:documentation>ИНН/КИО налогоплательщика</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="5258006800"/>
																											<altova:example value="771527131300"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="5"/>
																										<xs:maxLength value="12"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="КПППП" use="optional">
																								<xs:annotation>
																									<xs:documentation>КПП налогоплательщика</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="525801001"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:length value="9"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="НомСчПП" use="optional">
																								<xs:annotation>
																									<xs:documentation>Номер счета (специального банковского счета), идентификатор (номер) ЭСП в российском банке или в банке-нерезиденте</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="30101116062230010010"/>
																											<altova:example value="40802810500450000070"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:string">
																										<xs:minLength value="1"/>
																										<xs:maxLength value="34"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																						</xs:complexType>
																					</xs:element>
																					<xs:element name="СуммаОпер">
																						<xs:annotation>
																							<xs:documentation>Сумма операции (перевода) по счету, вкладу, депозиту (ЭДС)</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:attribute name="Дебет" use="required">
																								<xs:annotation>
																									<xs:documentation>По дебету (уменьшение)</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="34522.12"/>
																											<altova:example value="120345.10"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:decimal">
																										<xs:totalDigits value="18"/>
																										<xs:fractionDigits value="2"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																							<xs:attribute name="Кредит" use="required">
																								<xs:annotation>
																									<xs:documentation>По кредиту (увеличение)</xs:documentation>
																									<xs:appinfo>
																										<altova:exampleValues>
																											<altova:example value="342.12"/>
																											<altova:example value="1203.10"/>
																										</altova:exampleValues>
																									</xs:appinfo>
																								</xs:annotation>
																								<xs:simpleType>
																									<xs:restriction base="xs:decimal">
																										<xs:totalDigits value="18"/>
																										<xs:fractionDigits value="2"/>
																									</xs:restriction>
																								</xs:simpleType>
																							</xs:attribute>
																						</xs:complexType>
																					</xs:element>
																				</xs:sequence>
																				<xs:attribute name="ИдБлок" use="required">
																					<xs:annotation>
																						<xs:documentation>Идентификатор блока</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="1"/>
																								<altova:example value="2"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:nonNegativeInteger">
																							<xs:minInclusive value="1"/>
																							<xs:maxInclusive value="1000"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																				<xs:attribute name="ДатаОпер" type="xs:date" use="required">
																					<xs:annotation>
																						<xs:documentation>Дата совершения операции (перевода)</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="2010-03-15"/>
																								<altova:example value="2013-04-21"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																				</xs:attribute>
																				<xs:attribute name="НазнПл">
																					<xs:annotation>
																						<xs:documentation>Назначение платежа или код перевода</xs:documentation>
																						<xs:appinfo>
																							<altova:exampleValues>
																								<altova:example value="расходный кассовый ордер"/>
																								<altova:example value="1.2"/>
																							</altova:exampleValues>
																						</xs:appinfo>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:minLength value="1"/>
																							<xs:maxLength value="1000"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:complexType>
																		</xs:element>
																	</xs:choice>
																	<xs:attribute name="ПорНом" use="optional">
																		<xs:annotation>
																			<xs:documentation>Порядковый номер счета. Соответствует номеру LLLLLL в имени дополнительных файлов</xs:documentation>
																			<xs:appinfo>
																				<altova:exampleValues>
																					<altova:example value="000001"/>
																					<altova:example value="000002"/>
																				</altova:exampleValues>
																			</xs:appinfo>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:string">
																				<xs:length value="6"/>
																				<xs:pattern value="[0-9]{6}"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="НомСч" use="required">
																		<xs:annotation>
																			<xs:documentation>Номер счета (вклада, депозита, ЭДС)</xs:documentation>
																			<xs:appinfo>
																				<altova:exampleValues>
																					<altova:example value="40702ABC938090001069"/>
																					<altova:example value="938090001322"/>
																				</altova:exampleValues>
																			</xs:appinfo>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:string">
																				<xs:minLength value="1"/>
																				<xs:maxLength value="20"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="КодВал" use="optional">
																		<xs:annotation>
																			<xs:documentation>Цифровой код валюты в соответствии с Общероссийским классификатором валют</xs:documentation>
																			<xs:appinfo>
																				<altova:exampleValues>
																					<altova:example value="810"/>
																				</altova:exampleValues>
																			</xs:appinfo>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:decimal">
																				<xs:totalDigits value="3"/>
																				<xs:pattern value="\d{3}"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="ДатаНач" use="optional">
																		<xs:annotation>
																			<xs:documentation>Дата начала периода, за который представлена выписка</xs:documentation>
																			<xs:appinfo>
																				<altova:exampleValues>
																					<altova:example value="2014-04-25"/>
																				</altova:exampleValues>
																			</xs:appinfo>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:date"/>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="ДатаКон" use="optional">
																		<xs:annotation>
																			<xs:documentation>Дата конца периода, за который представлена выписка</xs:documentation>
																			<xs:appinfo>
																				<altova:exampleValues>
